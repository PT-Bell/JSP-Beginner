package ch04.com.dao;

public class Person {
	private int id=202014978;
	private String name="이윤종";
	
	public Person() {
		super();		
	}
	
	public int getId() {
		return id;
	}
	
	public void setId(int id) {
		this.id=id;

	}
	
	public String getName() {
		return name;
	}
	
	public void setName(String name) {
		this.name=name;
	}

}
